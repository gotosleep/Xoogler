{
    "name": "Xoogler Shortcuts",
    "description": "Useful shortcuts!",
    "version": "1.1",
    "manifest_version": 2,
    "icons": {
        "16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png",
        "512": "./icons/icon512.png"
    },
    "background": {
        "scripts": [
            "./js/map.js",
            "./js/background.js"
        ],
        "persistent": true
    },
    "options_page": "options.html",
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "*://b/",
        "*://c/",
        "*://cl/",
        "*://cs/",
        "*://docs/",
        "*://drive/",
        "*://go/",
        "*://m/",
        "*://moma/",
        "*://sheets/",
        "*://slides/",
        "*://who/"
    ]
}